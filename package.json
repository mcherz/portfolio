    "dot-prop-immutable": "^1.4.0",
    "elizabot": "^0.0.2",
    "react": "=16.2.0",
    "react-dom": "=16.2.0",
    "react-redux": "=5.0.6",
    "redux": "=3.7.2",
    "redux-thunk": "=2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
